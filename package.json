{
  "name": "citi",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "inquiry-capture:dev": "ng serve inquiry-capture --proxy-config=proxy.conf.dev.mjs",
    "inquiry-capture:uat": "ng serve inquiry-capture --proxy-config=proxy.conf.uat.mjs",
    "inquiry-blotter:dev": "ng serve Inquiry-blotter --proxy-config=proxy.conf.dev.mjs",
    "inquiry-blotter:uat": "ng serve Inquiry-blotter --proxy-config=proxy.conf.uat.mjs",
    "build": "ng build",
    "build:prod": "ng build --configuraiton production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "commit": "git-cz",
    "prepare": "husky",
    "lint": "ng lint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.12",
    "@angular/common": "^18.2.12",
    "@angular/compiler": "^18.2.12",
    "@angular/core": "^18.2.12",
    "@angular/forms": "^18.2.12",
    "@angular/platform-browser": "^18.2.12",
    "@angular/platform-browser-dynamic": "^18.2.12",
    "@angular/router": "^18.2.12",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.12",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "18.4.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.15.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.2.1",
    "typescript": "~5.5.4",
    "typescript-eslint": "8.15.0"
  }
}
